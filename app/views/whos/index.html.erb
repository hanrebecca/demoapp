<p id="notice"><%= notice %></p>

<h1>Listing Whos</h1>

<table>
  <thead>
    <tr>
      <th>Are</th>
      <th>You</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @whos.each do |who| %>
      <tr>
        <td><%= who.are %></td>
        <td><%= who.you %></td>
        <td><%= who.age %></td>
        <td><%= link_to 'Show', who %></td>
        <td><%= link_to 'Edit', edit_who_path(who) %></td>
        <td><%= link_to 'Destroy', who, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Who', new_who_path %>
